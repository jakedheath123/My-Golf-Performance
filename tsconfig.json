{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "Node",
    "esModuleInterop": true
  },
  "include": [
    "src"
  ],
  "exclude": ["node_modules"]
}